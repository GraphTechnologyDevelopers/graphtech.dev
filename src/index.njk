{% import "macros/renderNodeList.njk" as renderNodeList %}

<section class="hero hero--cta">
  <span class="hero__badge">// join the graph</span>
  <h2>Develop your graph brain in the X Community</h2>
  <p>Threads, pulses, and data drops daily. Share schemas, queries, datasets, and benchmarks. Get real-time feedback from Graph Technology Developers across the everything app.</p>
  <div class="hero__cta-group">
    <a class="btn-primary" href="https://x.com/i/communities/1977449294861881612" target="_blank" rel="noopener">Join the X community</a>
    <a class="btn-secondary" href="https://github.com/GraphTechnologyDevelopers" target="_blank" rel="noopener">Explore GitHub Org</a>
    <a class="btn-secondary" href="https://github.com/codegraphtheory" target="_blank" rel="noopener">Follow @codegraphtheory</a>
  </div>
</section>

<div class="graph-container" data-graph-root>
  <div class="graph-noise" aria-hidden="true"></div>
  <div class="graph-glyphs" aria-hidden="true"></div>
  <div class="graph-controls">
    <div class="graph-filter">
      <label for="graph-filter-input">Filter nodes</label>
      <input id="graph-filter-input" name="graph-filter" type="search" placeholder="Search topics, vendors, resources…" autocomplete="off">
    </div>
    <div class="legend" aria-label="Graph legend" role="group">
      {% include "partials/legend.njk" %}
    </div>
  </div>
  <svg class="graph-svg" role="img" aria-label="Graph navigation for Graph Technology Developers"></svg>
  <div class="graph-tooltip" role="status" aria-live="polite" hidden>
    <div class="graph-tooltip__label"></div>
    <div class="graph-tooltip__description"></div>
  </div>
  <div class="graph-fallback" aria-hidden="true"></div>
</div>

<section class="nodes-list" id="resources">
  <h2>All Resources</h2>
  <div class="nodes-list__content">
    <div class="nodes-list__group">
      <h3>X Community</h3>
      {% set xHub = graphData.nodes | findNodeById('ext_community') %}
      {% if xHub %}
        <ul>
          {% set depth = xHub.id | getNodeDepth(graphData) %}
          <li class="nodes-list__item" data-depth="{{ depth }}">
            <a href="{{ xHub.href }}" {% if xHub.href | isExternalLink %}target="_blank" rel="noopener"{% endif %}>{{ xHub.label }}</a>
            {% if xHub.description %}<p class="nodes-list__description">{{ xHub.description }}</p>{% endif %}
            {{ renderNodeList.renderNodeList(graphData.nodes, xHub.id, graphData) }}
          </li>
        </ul>
      {% endif %}
    </div>
    
    <div class="nodes-list__group">
      <h3>GitHub</h3>
      {% set githubHub = graphData.nodes | findNodeById('ext_github') %}
      {% if githubHub %}
        <ul>
          {% set depth = githubHub.id | getNodeDepth(graphData) %}
          <li class="nodes-list__item" data-depth="{{ depth }}">
            <a href="{{ githubHub.href }}" {% if githubHub.href | isExternalLink %}target="_blank" rel="noopener"{% endif %}>{{ githubHub.label }}</a>
            {% if githubHub.description %}<p class="nodes-list__description">{{ githubHub.description }}</p>{% endif %}
            {{ renderNodeList.renderNodeList(graphData.nodes, githubHub.id, graphData) }}
          </li>
        </ul>
      {% endif %}
    </div>
    
    <div class="nodes-list__group">
      <h3>Hub</h3>
      <ul>
        {% for node in graphData.nodes %}
          {% if not node.tags or ("x" not in node.tags and "github" not in node.tags) %}
          {% set depth = node.id | getNodeDepth(graphData) %}
          <li class="nodes-list__item" data-depth="{{ depth }}">
            <a href="{{ node.href }}" {% if node.href | isExternalLink %}target="_blank" rel="noopener"{% endif %}>{{ node.label }}</a>
            {% if node.description %}<p class="nodes-list__description">{{ node.description }}</p>{% endif %}
          </li>
          {% endif %}
        {% endfor %}
      </ul>
    </div>
  </div>
</section>

